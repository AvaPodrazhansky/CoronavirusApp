{
  "expo": {
    "name": "Anti-Coronavirus (COVID-19) Assistant",
    "slug": "anti-coronavirus-covid-19-assistant",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/images/appstore.png",
      "bundleIdentifier": "com.globalvirustracker.mobile",
      "supportsTablet": false
    },
    "android": {
      "icon": "./assets/images/playstore.png",
      "package": "com.globalvirustracker.mobile",
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
    }
  }
}
